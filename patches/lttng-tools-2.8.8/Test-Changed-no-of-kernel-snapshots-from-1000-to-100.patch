From: Anders Wallin <anders.wallin@windriver.com>
Date: Fri, 15 Sep 2017 11:08:24 +0200
Subject: [PATCH 1/1] Test: Changed no of kernel snapshots from 1000 to 100

Signed-off-by: Anders Wallin <anders.wallin@windriver.com>
---
 tests/regression/tools/snapshots/test_kernel | 10 +++++-----
 1 file changed, 5 insertions(+), 5 deletions(-)

diff --git a/tests/regression/tools/snapshots/test_kernel b/tests/regression/tools/snapshots/test_kernel
index 97874de8..d14fc016 100755
--- a/tests/regression/tools/snapshots/test_kernel
+++ b/tests/regression/tools/snapshots/test_kernel
@@ -25,7 +25,7 @@ CHANNEL_NAME="snapchan"
 
 TRACE_PATH=$(mktemp -d)
 
-NUM_TESTS=2040
+NUM_TESTS=240
 
 source $TESTDIR/utils/utils.sh
 
@@ -112,9 +112,9 @@ function test_kernel_local_snapshot_append_to_metadata ()
 	destroy_lttng_session_ok $SESSION_NAME
 }
 
-function test_kernel_1000_local_snapshots ()
+function test_kernel_100_local_snapshots ()
 {
-	NB_SNAP=1000
+	NB_SNAP=100
 
 	diag "Test local kernel snapshots"
 	create_lttng_session_no_output $SESSION_NAME
@@ -153,8 +153,8 @@ skip $isroot "Root access is needed. Skipping all kernel snapshot tests." $NUM_T
 {
 	start_lttng_sessiond
 
-	#tests=( test_kernel_1000_local_snapshots )
-	tests=( test_kernel_local_snapshot test_kernel_local_snapshot_after_stop test_kernel_local_snapshot_append_to_metadata test_kernel_1000_local_snapshots )
+	#tests=( test_kernel_100_local_snapshots )
+	tests=( test_kernel_local_snapshot test_kernel_local_snapshot_after_stop test_kernel_local_snapshot_append_to_metadata test_kernel_100_local_snapshots )
 
 	for fct_test in ${tests[@]};
 	do
